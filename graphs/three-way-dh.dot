digraph ThreeWayDH {
	ratio=fill;
 	size="3,3";
	node [shape=plaintext];
	A [color=white fontcolor=white];
	B [color=white fontcolor=white];
	a [color=white fontcolor=white];
	b [color=white fontcolor=white];
